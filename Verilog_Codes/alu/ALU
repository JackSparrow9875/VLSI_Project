    .port_info 1 /INPUT 1 "S1";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 4 "y";
v0x55edf15a50f0_0 .net "S0", 0 0, v0x55edf15f5a00_0;  1 drivers
v0x55edf15f56e0_0 .net "S1", 0 0, v0x55edf15f5ac0_0;  1 drivers
