    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55edf15f56e0_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55edf15f5870_0, 0, 4;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55edf15a50f0_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55edf15f56e0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55edf15f5870_0, 0, 4;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x55edf15a50f0_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55edf15f56e0_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55edf15f5870_0, 0, 4;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x55edf15f5870_0, 0, 4;
T_0.7 ;
T_0.5 ;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55edf15f5870_0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55edf15e0010;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55edf15f5a00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55edf15f5ac0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55edf15f5b90_0, 0, 1;
    %vpi_call 2 28 "$dumpfile", "ALU.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55edf15e0010 {0 0 0};
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55edf15f5a00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55edf15f5ac0_0, 0, 1;
